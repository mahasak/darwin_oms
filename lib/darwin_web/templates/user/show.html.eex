<h1>Show User</h1>

<ul>

  <li>
    <strong>Login name:</strong>
    <%= @user.login_name %>
  </li>

  <li>
    <strong>Is active:</strong>
    <%= @user.is_active %>
  </li>

  <li>
    <strong>Is deleted:</strong>
    <%= @user.is_deleted %>
  </li>

</ul>

<span><%= link "Edit", to: Routes.user_path(@conn, :edit, @user) %></span>
<span><%= link "Back", to: Routes.user_path(@conn, :index) %></span>
